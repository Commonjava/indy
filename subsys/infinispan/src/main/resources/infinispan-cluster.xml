<?xml version="1.0" encoding="UTF-8"?>
<infinispan xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="urn:infinispan:config:9.4 http://www.infinispan.org/schemas/infinispan-config-9.4.xsd"
            xmlns="urn:infinispan:config:9.4">

  <jgroups>
    <stack-file name="external-file" path="jgroups-tcp.xml" />
  </jgroups>

  <cache-container default-cache="event-audit" name="IndyClusterCacheManager" shutdown-hook="DEFAULT" statistics="true">
    <transport stack="external-file" cluster="default"/>

    <replicated-cache name="event-audit"/>

  </cache-container>
</infinispan>
